{
  "name": "angular-boilerplate",
  "version": "0.0.0",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "ng": "ng",
    "dev": "ng serve -o",
    "start": "ng serve",
    "start:es": "ng serve -c=es --port 4201",
    "build": "ng build",
    "build:prod": "ng build --prod --build-optimizer --aot --stats-json",
    "build:i18n": "ng build --prod --build-optimizer --aot --stats-json --localize",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n:extract": "ng extract-i18n --output-path locale --ivy",
    "analyze": "webpack-bundle-analyzer dist/angular-boilerplate/stats.json"
  },
  "dependencies": {
    "@angular-eslint/builder": "^2.0.2",
    "@angular-eslint/template-parser": "^2.0.2",
    "@angular/animations": "~11.2.0",
    "@angular/common": "~11.2.0",
    "@angular/compiler": "~11.2.0",
    "@angular/core": "~11.2.0",
    "@angular/forms": "~11.2.0",
    "@angular/platform-browser": "~11.2.0",
    "@angular/platform-browser-dynamic": "~11.2.0",
    "@angular/router": "~11.2.0",
    "@angular/service-worker": "~11.2.0",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.8",
    "rxjs": "~6.6.6",
    "tailwindcss": "^2.0.4",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular-eslint/eslint-plugin": "^2.0.2",
    "@angular-eslint/eslint-plugin-template": "^2.0.2",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.0",
    "@angular/localize": "^11.2.0",
    "@types/jasmine": "~3.6.7",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-angular": "^4.0.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "typescript": "^4.1.5",
    "webpack-bundle-analyzer": "^4.4.0"
  }
}
